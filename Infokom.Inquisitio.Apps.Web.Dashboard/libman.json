{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "datatables.net@2.1.8",
      "destination": "wwwroot/lib/datatables.net/"
    },
    {
      "library": "sweetalert2@11.14.5",
      "destination": "wwwroot/lib/sweetalert2/"
    },
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "bootstrap-icons@1.11.3",
      "destination": "wwwroot/lib/bootstrap-icons/"
    },
    {
      "library": "Chart.js@4.4.1",
      "destination": "wwwroot/lib/Chart.js/"
    },
    {
      "library": "chartjs-plugin-datalabels@2.2.0",
      "destination": "wwwroot/lib/chartjs-plugin-datalabels/"
    },
    {
      "provider": "unpkg",
      "library": "sortablejs@1.15.3",
      "destination": "wwwroot/lib/sortablejs/",
      "files": [
        "Sortable.js",
        "Sortable.min.js"
      ]
    }
  ]
}